{
    "Statuses": {
        "Backlog": {
            "title": "Sprint Backlog",
            "display": "column"
        },
        "In Progress": {
            "display": "row"
        },
        "Blocked": {
            "display": "row"
        },
        "QA": {
            "display": "row",
            "request": [
                {"id":"time_spent", "name":"Time Spent (hours)", "type":"text"},
                {"id":"completion_notes", "name":"Work Summary", "type":"textarea"}
            ]
        },
        "Done": {
            "display": "row",
            "limit_drag_to": [
                "QA"
            ]
        },
        "Derpricated": {
